*******************************MY BRUTE FORCE GAVE WRONG ANSWER****************************************
#include<bits/stdc++.h>
#include <iostream>
using namespace std;
bool ispossible(int*a,int i,int j){
    int p1= (a[i]-a[j])/a[i];
    int p2= (a[i]-a[j])/a[j];
    if(p1<p2)return true;
    else
    return false;
}

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int count=0;
	    int n;
	    int arr[n];
	    for(int i=0;i<n;i++){
	        cin>>arr[i];
	    }
	    
	    for(int i=1;i<=n;i++){
	        
	        for(int j=1;j<=n;j++){
	        if(i!=j){
	            if(ispossible(arr,i-1,j-1))count++;
	        }
	        }
	    }
	    cout<<count<<endl;
	}
	return 0;
}
************************************************THE OPTIMAL AND BEST ANSWER***********************************************************
TIME:O(N)

