PROBLEM:https://www.codechef.com/START27D/problems/BLACKJACK

#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int a,b;
	    cin>>a>>b;
	    int third_no=21-a-b;
	    if(1<=third_no && third_no<=10){
	        cout<<third_no<<endl;
	    }
	    else{
	        cout<<-1<<endl;
	    }
	}
	return 0;
}

