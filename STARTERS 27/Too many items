problem:https://www.codechef.com/START27D/problems/POLYBAGS
//brute force
#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
{
    int n;
    cin>>n;
    if(n%10==0){
        cout<<n/10<<endl;
    }
    else{
        cout<<(n/10)+1<<endl;
    }
}
	return 0;
}
//   A BETTER WAY TO WRITE THE ABOVE CODE
#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
{
    int n;
    cin>>n;
    cout<<(n%10==0 ? n/10 :n/10 +1)<<endl;
}

	return 0;
}

//A SMARTER APPROACH
#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
{
    int n;
    cin>>n;
    cout<<(n+9)/10<<endl;
}

	return 0;
}

